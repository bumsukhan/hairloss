#### 게시판 테이블 ####
CREATE TABLE HAIR_BOARD 
(
  BNUM NUMBER NOT NULL 
, TITLE VARCHAR2(4000 BYTE) NOT NULL 
, CONTENTS VARCHAR2(4000 BYTE) NOT NULL 
, REGDATE DATE NOT NULL 
, USERS_ID VARCHAR2(20 BYTE) NOT NULL 
, DEL_YN CHAR(1 BYTE) NOT NULL 
, CNT NUMBER 
, CONSTRAINT PK_BOARD PRIMARY KEY 
  (
    BNUM 
  )
  USING INDEX 
  (
      CREATE UNIQUE INDEX PK_BOARD ON HAIR_BOARD (BNUM ASC) 
      LOGGING 
      TABLESPACE USERS 
      PCTFREE 10 
      INITRANS 2 
      STORAGE 
      ( 
        INITIAL 65536 
        NEXT 1048576 
        MINEXTENTS 1 
        MAXEXTENTS UNLIMITED 
        BUFFER_POOL DEFAULT 
      ) 
      NOPARALLEL 
  )
  ENABLE 
) 
LOGGING 
TABLESPACE USERS 
PCTFREE 10 
INITRANS 1 
STORAGE 
( 
  INITIAL 65536 
  NEXT 1048576 
  MINEXTENTS 1 
  MAXEXTENTS UNLIMITED 
  BUFFER_POOL DEFAULT 
) 
NOCOMPRESS 
NOPARALLEL;

COMMENT ON COLUMN HAIR_BOARD.BNUM IS '게시판번호';
COMMENT ON COLUMN HAIR_BOARD.TITLE IS '제목';
COMMENT ON COLUMN HAIR_BOARD.CONTENTS IS '내용';
COMMENT ON COLUMN HAIR_BOARD.REGDATE IS '작성일';
COMMENT ON COLUMN HAIR_BOARD.USERS_ID IS '아이디';
COMMENT ON COLUMN HAIR_BOARD.DEL_YN IS '사용여부';
COMMENT ON COLUMN HAIR_BOARD.CNT IS '조회수';


