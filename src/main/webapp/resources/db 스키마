#### HAIR_FILE ####
CREATE TABLE HAIR_FILE 
(
  BNUM NUMBER 
, ORIGINAL_FILE_NAME VARCHAR2(260 BYTE) NOT NULL 
, STORED_FILE_NAME VARCHAR2(36 BYTE) NOT NULL 
, FILE_SIZE NUMBER 
, USERS_DTM DATE DEFAULT sysdate NOT NULL 
, USERS_ID VARCHAR2(30 BYTE) NOT NULL 
, DEL_GB VARCHAR2(1 BYTE) DEFAULT 'N' NOT NULL 
, FNUM NUMBER NOT NULL 
, CONSTRAINT FNUM PRIMARY KEY 
  (
    FNUM 
  )
  USING INDEX 
  (
      CREATE UNIQUE INDEX SYS_C0011379 ON HAIR_FILE (FNUM ASC) 
      LOGGING 
      TABLESPACE USERS 
      PCTFREE 10 
      INITRANS 2 
      STORAGE 
      ( 
        BUFFER_POOL DEFAULT 
      ) 
      NOPARALLEL 
  )
  ENABLE 
) 
LOGGING 
TABLESPACE USERS 
PCTFREE 10 
INITRANS 1 
STORAGE 
( 
  BUFFER_POOL DEFAULT 
) 
NOCOMPRESS 
NOPARALLEL;

#### HAIR_USERS ####
CREATE TABLE HAIR_USERS 
(
  USERS_ID VARCHAR2(20 BYTE) NOT NULL 
, USERS_PW VARCHAR2(20 BYTE) NOT NULL 
, USERS_ROLE NUMBER NOT NULL 
, USERS_NAME VARCHAR2(20 BYTE) NOT NULL 
, USERS_JUMIN VARCHAR2(20 BYTE) NOT NULL 
, USERS_EMAIL VARCHAR2(50 BYTE) NOT NULL 
, USERS_PHONE_NUMBER VARCHAR2(50 BYTE) NOT NULL 
, CONSTRAINT PK_USERS PRIMARY KEY 
  (
    USERS_ID 
  )
  USING INDEX 
  (
      CREATE UNIQUE INDEX PK_USERS ON HAIR_USERS (USERS_ID ASC) 
      LOGGING 
      TABLESPACE USERS 
      PCTFREE 10 
      INITRANS 2 
      STORAGE 
      ( 
        INITIAL 65536 
        NEXT 1048576 
        MINEXTENTS 1 
        MAXEXTENTS UNLIMITED 
        BUFFER_POOL DEFAULT 
      ) 
      NOPARALLEL 
  )
  ENABLE 
) 
LOGGING 
TABLESPACE USERS 
PCTFREE 10 
INITRANS 1 
STORAGE 
( 
  INITIAL 65536 
  NEXT 1048576 
  MINEXTENTS 1 
  MAXEXTENTS UNLIMITED 
  BUFFER_POOL DEFAULT 
) 
NOCOMPRESS 
NOPARALLEL;

COMMENT ON COLUMN HAIR_USERS.USERS_ID IS '아이디';
COMMENT ON COLUMN HAIR_USERS.USERS_PW IS '비밀번호';
COMMENT ON COLUMN HAIR_USERS.USERS_ROLE IS '권한';
COMMENT ON COLUMN HAIR_USERS.USERS_NAME IS '이름';
COMMENT ON COLUMN HAIR_USERS.USERS_JUMIN IS '주민등록번호';
COMMENT ON COLUMN HAIR_USERS.USERS_EMAIL IS '이메일';
COMMENT ON COLUMN HAIR_USERS.USERS_PHONE_NUMBER IS '핸드폰';

